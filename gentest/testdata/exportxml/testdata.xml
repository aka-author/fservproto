<?xml version="1.0" encoding="UTF-8"?>
<!-- 
    Binding tables we export from the testdata database 
    into a single XML file  
-->
<!DOCTYPE testDatabase [
    <!ENTITY genres_aspects SYSTEM "genres_aspects.xml">
    <!ENTITY online_docs SYSTEM "online_docs.xml">
    <!ENTITY topics SYSTEM "topics.xml">
    <!ENTITY product_groups SYSTEM "product_groups.xml">
    <!ENTITY product_subgroups SYSTEM "product_subgroups.xml">
    <!ENTITY technologies SYSTEM "technologies.xml">
    <!ENTITY subjects SYSTEM "subjects.xml">
    <!ENTITY gtopics_subjects SYSTEM "gtopics_subjects.xml">
    
]>
<database>
    <genres_aspects>&genres_aspects;</genres_aspects>
    <online_docs>&online_docs;</online_docs>
    <topics>&topics;</topics>
    <taxonomy>
        <product_groups>&product_groups;</product_groups>
        <product_subgroups>&product_subgroups;</product_subgroups>
        <technologies>&technologies;</technologies>
        <subjects>&subjects;</subjects>
        <gtopics_subjects>&gtopics_subjects;</gtopics_subjects>
    </taxonomy>
</database>
